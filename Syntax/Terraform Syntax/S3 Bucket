provider "aws" {
  region = "us-west-2"  # Replace with your desired AWS region
}

resource "aws_s3_bucket" "example" {
  bucket = "my-example-bucket"  # Replace with a unique bucket name

  # Optional settings
  acl    = "private"            # Access control list for the bucket
  tags = {
    Name = "example-bucket"
  }
}
